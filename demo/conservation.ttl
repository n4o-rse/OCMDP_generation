        "Stephan Patscher",
        "Ulrike Lehnert",
        "Waldemar Muskalla" ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:publisher "Leibniz-Zentrum für Archäologie (LEIZA)" ;
    dcterms:rights "CC BY 4.0" ;
